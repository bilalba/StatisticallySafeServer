<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Safex</servlet-name>
		<servlet-class>edu.asu.cse546.SafeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Safex</servlet-name>
		<url-pattern>/safe</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Safe</servlet-name>
		<servlet-class>edu.asu.cse546.getOverview</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Safe</servlet-name>
		<url-pattern>/getOverview</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Near</servlet-name>

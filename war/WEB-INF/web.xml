		<servlet-name>Near</servlet-name>
		<url-pattern>/getNearest</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Time</servlet-name>
		<servlet-class>edu.asu.cse546.getTimeData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Time</servlet-name>
		<url-pattern>/getTimeData</url-pattern>
	</servlet-mapping>
	

	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
